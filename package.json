{
  "name": "extension-backup",
  "version": "0.0.1",
  "description": "do you want a extension to backup your all of your vs code setting just by using you gmail? here is the extension that do it !!! ",
  "homepage": "https://github.com/arman-danesh/extension-backup#readme",
  "bugs": {
    "url": "https://github.com/arman-danesh/extension-backup/issues"
  },
  "icon": "asset/icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arman-danesh/extension-backup.git"
  },
  "license": "ISC",
  "author": "arman danesh",
  "type": "commonjs",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "vsce": "vsce package",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test && npm run copy-css",
    "copy-css": "cpy \"src/ui/*.css\" \"out/ui/\"",
    "compile": "tsc -p ./ && npm run copy-css",
    "watch": "tsc -watch -p ./ & npm run copy-css"
  },
  "dependencies": {
    "googleapis": "^109.0.0",
    "nodemailer": "^7.0.5",
    "server-destroy": "^1.0.1",
    "vsce": "^2.15.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/nodemailer": "^7.0.1",
    "@types/server-destroy": "^1.0.4",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "displayName": "Extension Backup",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:backup.uploadGoogle",
    "onCommand:backup.downloadGoogle",
    "onCommand:backup.showUI"
  ],
  "contributes": {
    "commands": [
      {
        "command": "backup.uploadGoogle",
        "title": "Backup: Upload Google Drive"
      },
      {
        "command": "backup.downloadGoogle",
        "title": "Backup: Download from Google Drive"
      },
      {
        "command": "backup.showUI",
        "title": "Backup: Open Backup Manager UI"
      }
    ]
  }
}
